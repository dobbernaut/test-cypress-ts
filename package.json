{
  "name": "test-cypress-ts",
  "version": "0.0.1",
  "description": "cypress test framework poc with typescript",
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "sh ./scripts/preinstall.sh",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
