    "eslint-plugin-cypress": "^2.11.3",
    "har-validator": "^5.1.5",
    "junit-report-merger": "^3.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
